{
  "name": "JaSON",
  "version": "5.0.0",
  "manifest_version": 2,
  "description": "JaSON: JSON and XML API test tool",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "JaSON",
    "default_icon": {
      "128": "jason-128x128.png",
      "256": "jason-256x256.png",
      "512": "jason-256x256.png"
    }
  },
  "icons": {
    "128": "jason-128x128.png",
    "256": "jason-256x256.png",
    "512": "jason-256x256.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}